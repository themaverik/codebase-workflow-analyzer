{
  "project_types": [
    {
      "id": "analysis_tool",
      "name": "Analysis Tool",
      "description": "Codebase analyzers, linters, formatters, code intelligence tools",
      "indicators": [
        {
          "type": "cargo_binary_name",
          "patterns": ["analyzer", "linter", "formatter", "checker", "parser"]
        },
        {
          "type": "dependency_pattern",
          "patterns": ["tree-sitter", "syn", "swc", "eslint", "ast"]
        },
        {
          "type": "cli_structure",
          "patterns": ["clap", "structopt", "main.rs"]
        },
        {
          "type": "readme_keywords",
          "patterns": ["analyze", "lint", "format", "parse", "static analysis", "code intelligence"]
        }
      ],
      "analysis_focus": "tool capabilities and analysis features",
      "domain_hints": ["Code Analysis", "Developer Tools", "Static Analysis"]
    },
    {
      "id": "web_application",
      "name": "Web Application",
      "description": "Frontend/backend web apps, SPAs, progressive web apps",
      "indicators": [
        {
          "type": "package_dependencies",
          "patterns": ["react", "vue", "angular", "next", "nuxt", "express", "koa"]
        },
        {
          "type": "file_structure",
          "patterns": ["src/components", "pages", "public", "static", "templates"]
        },
        {
          "type": "config_files",
          "patterns": ["webpack.config.js", "vite.config.js", "next.config.js"]
        }
      ],
      "analysis_focus": "user-facing features and business logic",
      "domain_hints": ["User Interface", "Business Logic", "Web Services"]
    },
    {
      "id": "api_service",
      "name": "API Service",
      "description": "REST/GraphQL API backends, microservices",
      "indicators": [
        {
          "type": "framework_patterns",
          "patterns": ["fastapi", "flask", "django", "spring-boot", "nestjs", "axum", "actix"]
        },
        {
          "type": "route_patterns",
          "patterns": ["/api/", "router", "controller", "endpoint"]
        },
        {
          "type": "database_patterns",
          "patterns": ["orm", "database", "migrations", "models"]
        }
      ],
      "analysis_focus": "API endpoints and data processing",
      "domain_hints": ["API Design", "Data Processing", "Backend Services"]
    },
    {
      "id": "cli_tool",
      "name": "CLI Tool",
      "description": "Command-line utilities, terminal applications",
      "indicators": [
        {
          "type": "cli_dependencies",
          "patterns": ["clap", "structopt", "click", "argparse", "commander"]
        },
        {
          "type": "binary_config",
          "patterns": ["[[bin]]", "console_scripts", "scripts"]
        }
      ],
      "analysis_focus": "command-line interface and automation capabilities",
      "domain_hints": ["Automation", "Command Line", "System Tools"]
    },
    {
      "id": "library",
      "name": "Library",
      "description": "Reusable code libraries, SDKs, frameworks",
      "indicators": [
        {
          "type": "library_structure",
          "patterns": ["lib.rs", "__init__.py", "index.ts", "src/lib"]
        },
        {
          "type": "package_type",
          "patterns": ["library", "package", "crate-type = \"lib\""]
        }
      ],
      "analysis_focus": "provided abstractions and APIs",
      "domain_hints": ["Code Reusability", "API Design", "Software Architecture"]
    },
    {
      "id": "desktop_application",
      "name": "Desktop Application",
